{
  "name": "svelte-movable",
  "version": "1.1.3",
  "description": "Action to move node on mousedown & mousemove",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "svelte-kit dev",
    "lint:es": "eslint --ignore-path .gitignore .",
    "format": "prettier --check --ignore-path .gitignore --plugin-search-dir=. \"./**/*.{ts,js,cjs,json,svelte,css}\"",
    "build": "rollup -c",
    "semantic-release": "semantic-release",
    "api:extract": "api-extractor run --local --verbose",
    "api:document": "api-documenter markdown -i api/temp -o api/docs",
    "api": "run-s api:extract api:document"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnphanquang/svelte-action-movable.git"
  },
  "keywords": [
    "svelte",
    "action",
    "drag",
    "move",
    "movable",
    "modal",
    "dialog",
    "draggable",
    "mousedown"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnphanquang/svelte-action-movable/issues"
  },
  "homepage": "https://github.com/vnphanquang/svelte-action-movable#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.17.12",
    "@microsoft/api-extractor": "^7.23.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "1.0.0-next.326",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^3.1.4",
    "postcss-nesting": "^10.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "rollup": "^2.72.1",
    "rollup-plugin-filesize": "^9.1.2",
    "semantic-release": "^19.0.2",
    "svelte": "^3.44.0",
    "svelte-awesome": "^3.0.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.5",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
