{
  "name": "svelte-movable",
  "version": "1.0.0",
  "description": "Action to move node on mousedown",
  "main": "lib/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint:es": "eslint --ignore-path .gitignore .",
    "format": "prettier --check --config ./prettierrc.yaml .",
    "build": "tsc --project tsconfig.json",
    "semantic-release": "semantic-release",
    "api:extract": "api-extractor run --local --verbose",
    "api:document": "api-documenter markdown -i api/temp -o api/docs",
    "api": "run-s api:extract api:document"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnphanquang/svelte-action-movable.git"
  },
  "keywords": [
    "svelte",
    "action",
    "drag",
    "move",
    "movable",
    "modal",
    "dialog",
    "draggable",
    "mousedown"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnphanquang/svelte-action-movable/issues"
  },
  "homepage": "https://github.com/vnphanquang/svelte-action-movable#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.17.12",
    "@microsoft/api-extractor": "^7.23.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
